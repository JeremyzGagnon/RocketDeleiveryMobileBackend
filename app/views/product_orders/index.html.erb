<p style="color: green"><%= notice %></p>

<h1>Product Orders</h1>

<p style="color: green"><%= notice %></p>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Product Quantity </th>
      <th scope="col">Product Unit Cost </th>
    </tr>
  </thead>
  <tbody>
  <% @product_orders.each do |product_order| %>
    <tr>
      <td><%= product_order.product_quantity%></td>
      <td><%= product_order.product_unit_cost%></td>


      <td><%= link_to 'Show', product_order %></td>
      <td><%= link_to 'Edit', edit_product_order(product_order) %></td>
    <tr>
    <% end %>
  </tbody>
</table>

<br>

