<div id="<%= dom_id employee %>">
  <p>
    <strong>User:</strong>
    <%= employee.user_id %>
  </p>

  <p>
    <strong>Adress:</strong>
    <%= employee.address_id %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= employee.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= employee.email %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= employee.created_at %>
  </p>

  <p>
    <strong>Updated at:</strong>
    <%= employee.updated_at %>
  </p>

</div>
